{
  "name": "rustwasm-gif-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cargo": "cargo build --target wasm32-unknown-unknown --release",
    "bindgen": "wasm-bindgen target/wasm32-unknown-unknown/release/gif.wasm --target bundler --out-dir ./pkg",
    "build-wasm": "npm run cargo && npm run bindgen",
    "build-wasm-pack": "wasm-pack build -t bundler"
  },
  "devDependencies": {
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-top-level-await": "^1.2.1",
    "vite-plugin-wasm": "^3.1.0"
  },
  "dependencies": {
    "@types/events": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "events": "^3.3.0",
    "gif": "file:pkg",
    "uuid": "^9.0.0"
  }
}
